{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "web-build",
        "installCommand": "npm install",
        "buildCommand": "npx expo export:web"
      }
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/logo(.*).png",
      "dest": "/logo$1.png"
    },
    {
      "src": "^/(.*)\\.(js|json|css|map|ico|png|jpg|jpeg|svg|webp|gif|woff|woff2|ttf|eot|otf)",
      "dest": "/$1.$2"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
} 